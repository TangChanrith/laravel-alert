{
    "name": "chanr1th/laravel-alert",
    "description": "A Bootstrap alert helper for Laravel",
    "license": "MIT",
    "keywords": ["laravel", "bootstrap", "alert", "flash"],
    "authors": [
        {
            "name": "Tang Chanrith",
            "email": "tang.chanrith@gmail.com.com",
            "homepage": "http://chanrith.me"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/contracts": "5.6.*",
        "illuminate/session": "5.6.*",
        "illuminate/support": "5.6.*"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^2.0",
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Chanr1th\\Alert\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Chanr1th\\Tests\\Alert\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        },
        "laravel": {
            "providers": [
                "Chanr1th\\Alert\\AlertServiceProvider"
            ],
            "aliases": {
                "Alert": "Chanr1th\\Alert\\Facades\\Alert"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
