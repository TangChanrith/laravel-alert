{
    "name": "vinkla/alert",
    "description": "An alert helper for Laravel",
    "license": "MIT",
    "keywords": ["laravel", "bootstrap", "alert", "flash"],
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com"
        }
    ],
    "require": {
        "php": "^5.5.9 || ^7.0",
        "illuminate/session": "5.1.* || 5.2.*",
        "illuminate/support": "5.1.* || 5.2.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^4.8 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Vinkla\\Alert\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Vinkla\\Tests\\Alert\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
